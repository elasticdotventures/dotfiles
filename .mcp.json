{
  "mcpServers": {
    "b00t-mcp": {
      "args": [
        "--stdio"
      ],
      "command": "b00t-mcp"
    },
    "context7": {
      "args": [
        "-y",
        "@upstash/context7-mcp"
      ],
      "command": "bunx"
    },
    "github": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ],
      "command": "npx"
    },
    "taskmaster-ai": {
      "args": [
        "-y",
        "--package=task-master-ai",
        "task-master-ai"
      ],
      "command": "bunx",
      "env": {}
    }
  }
}