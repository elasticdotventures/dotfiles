{
  "name": "b00t-browser-ext",
  "displayName": "b00t Browser Extension",
  "version": "0.1.0",
  "description": "b00t browser extension for operator telemetry and agent integration",
  "author": "PromptExecution <hello@promptexecution.com>",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "plasmo": "^0.90.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@plasmohq/prettier-plugin-sort-imports": "^4.0.1",
    "@types/chrome": "^0.0.268",
    "@types/node": "^20.16.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4"
  },
  "manifest": {
    "permissions": [
      "activeTab",
      "webRequest",
      "storage",
      "tabs"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
}